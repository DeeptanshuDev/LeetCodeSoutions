Linear Search

#include<iostream>
using namespace std;

bool search(int arr[], int n, int searchKey){
	for(int i = 0; i < n; i++){
		if(arr[i] == searchKey){
			return true;
		}
	}
	return false;
}

int main() {
	int arr[10] = { 5, 7, -2, 10, 22, -2, 0, 5, 22, 10};
	
	if(search(arr, 10, 1)){
		cout<<"1 is present "<<endl;
	} else {
		cout<<"1 is not present "<<endl;
	}
	return 0;
}
